@import 'assets/css/normalize.css'

$color-text = #666
$color-high-text = #ff5777
$color-tint = #ff8198
$color-backgroundL = #fff
$font-size = 14px
$line-height = 1.5

*
	&::before
	&::after
		margin 0
		padding 0
		box-sizing border-box
body
	user-select none
	background $color-backgroundL
	color $color-text
	width 100vw
a
	color $color-text
	text-decoration none
.clear-fix
	zoom 1
	&::after
		clear both
		content ''
		display block
		width 0
		height 0
		visibility hidden
.left
	float left
.right
	float right
li 
	list-style none
	float left

	